{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": { "text": "Map showing the distribution of different car crash fatalities in australia" },
    "projection": {"type": "equalEarth"},
    "data":{
        "url": "https://raw.githubusercontent.com/trung1411/Data-visualisation-2/main/ne_110m.json",
        "format": {"type": "topojson", "feature": "ne_110m"}
    },
    "mark": {"type": "geoshape", "fill": "lightgray" , "stroke": "white"},
    "transform": [
        {
            "lookup": "properties.NAME",
            "from":{
                "data": {
                    "url": "https://raw.githubusercontent.com/trung1411/Data-visualisation-2/main/ardd_fatalities.csv"
                },
                "key": "State",
                "fields": []
            }
        }
    ]
        
            

    

}
    
  